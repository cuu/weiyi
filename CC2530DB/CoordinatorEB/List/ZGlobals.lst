    372          /*********************************************************************
    373           * @fn       zgItemInit()
    374           *
    375           * @brief
    376           *
    377           *   Initialize a global item. If the item doesn't exist in NV memory,
    378           *   write the system default (value passed in) into NV memory. But if
    379           *   it exists, set the item to the value stored in NV memory.
    380           *
    381           *   Also, if setDefault is TRUE and the item exists, we will write
    382           *   the default value to NV space.
    383           *
    384           * @param   id - item id
    385           * @param   len - item len
    386           * @param   buf - pointer to the item
    387           * @param   setDefault - TRUE to set default, not read
    388           *
    389           * @return  ZSUCCESS if successful, NV_ITEM_UNINIT if item did not
    390           *          exist in NV, NV_OPER_FAILED if failure.
    391           */
    392          static uint8 zgItemInit( uint16 id, uint16 len, void *buf, uint8 setDefault )
    393          {
    394            uint8 status;
    395          
    396            // If the item doesn't exist in NV memory, create and initialize
    397            // it with the value passed in.
    398            status = osal_nv_item_init( id, len, buf );
    399            if ( status == ZSUCCESS )
    400            {
    401              if ( setDefault )
    402              {
    403                // Write the default value back to NV
    404                status = osal_nv_write( id, 0, len, buf );
    405              }
    406              else
    407              {
    408                // The item exists in NV memory, read it from NV memory
    409                status = osal_nv_read( id, 0, len, buf );
    410              }
    411            }
    412          
    413            return (status);
    414          }
